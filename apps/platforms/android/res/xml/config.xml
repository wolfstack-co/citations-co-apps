<?xml version='1.0' encoding='utf-8'?>
<widget id="co.wolfstack.citations" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Canvas2ImagePlugin">
        <param name="android-package" value="org.devgeeks.Canvas2ImagePlugin.Canvas2ImagePlugin" />
    </feature>
    <feature name="Share">
        <param name="android-package" value="co.wolfstack.share.Share" />
    </feature>
    <name>Citations</name>
    <description>
        Citations.co HTML5 [cordova] apps by Wolfstack
    </description>
    <author email="tpillardpro@gmail.com" href="http://cordova.io">
        Wolfstack Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
